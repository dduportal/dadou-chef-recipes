<% def get_nginx_directive(key, value)
	content = "#{key} "
	if value.kind_of?(Hash)
		content += "{\n"
		value.each do | subKey, subValue |
			content += "\t" + get_nginx_directive(subKey,subValue)
		end
		content += "}\n"
	elsif value.kind_of?(Array)
		value.each do |subValue|
			if subValue.kind_of?(Hash)
				content += "{\n\t" 
				subValue.each do |subKey, subValue|
					content += "\t" + get_nginx_directive(subKey,subValue)
				end
				content += "}\n"
			end
		end
	else
		content += "#{value};\n"
	end
	return content
end %>


<% @nginx_conf.each do | key, value | 
	%><%= get_nginx_directive(key,value) %><% 
end %>
