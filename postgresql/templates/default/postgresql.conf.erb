# -----------------------------
# PostgreSQL configuration file
# This file was automatically generated and dropped off by chef so all modifications would be lost
# -----------------------------
#

<% node['postgresql']['server']['config'].sort.each do |key, value| %>
<% next if value.nil? -%>
<%= key %> = <%=
  case value
  when String
    "'#{value}'"
  when TrueClass
    'on'
  when FalseClass
    'off'
  else
    value
  end
%>
<% end %>